    "plot": {
        "mask": "dbg_raw_msk",
        "plots": [
            {
                // "color": ["black", "red", "green"]
                "fmt": ["I1.fffffffH", "N", "sq in", "sd in", "sq tgt", "sq i_term", "sd i_term", "sq cal", "sd cal", "sen_encoder"],
                //"fmt": ["I1.ffffffffff", "N", "v_a_real", "v_b_real", "i_a_real", "i_b_real", "i_a", "i_b", "e_a", "e_b", "atan2", "sen_elec"],
                "cal": {
                    "sq_err": "_d[1].at(-1) - _d[3].at(-1)", // data1 - data3
                    "sq_err_avg": "                  \
                        let a = 0;                   \
                        for (let i = 0; i < 5; i++)  \
                            a += _d[9].at(-1-i);     \
                        return a / 5;",
                    "sq_err_avg2": "                        \
                        if (_d[9]._f == undefined)          \
                            _d[9]._f = 0;                   \
                        let err = _d[9].at(-1) - _d[9]._f;  \
                        _d[9]._f += err * 0.02;             \
                        return _d[9]._f;"
                }
            }, {
                "fmt": ["I5.fffiHf", "N", "sen_speed", "speed target", "i_term", "cal_current", "sen_encoder", "sen_speed_avg"],
                "cal": {
                    "speed_err2i": "_d[2].at(-1) - _d[1].at(-1)",
                    "speed_err2p": "_d[2].at(-1) - _d[6].at(-1)",
                    "p term": "_d[4].at(-1) - _d[3].at(-1)"
                }
            }, {
                "fmt": ["I25.iiffff", "N", "pos_cur", "pos target", "i_term", "cal_speed", "tc_vc_avg", "sen_speed_avg"],
                "cal": {
                    "pos_err": "_d[2].at(-1) - _d[1].at(-1)",
                    "p term": "_d[7].at(-1) * 50"
                }
            }, {
                "fmt": ["I25.Biiff", "N", "tc_state", "tc_poc", "cal_pos", "tc_vc", "tc_ac"],
            }
        ]
    },
    
